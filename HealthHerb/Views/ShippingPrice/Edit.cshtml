@model ShippingPrice
@{
    Layout = "_AdminDashboard";
    ViewData["Title"] = "Edit Shipping";
}

<partial name="~/Views/Shared/_Alert.cshtml" />
<div class="row mt">
    <div class="col-lg-12">
        <h4><i class="fa fa-truck"></i> Edit Shipping Price</h4>
        <div class="form-panel">
            <form class="form-horizontal style-form" enctype="multipart/form-data" method="post" asp-controller="shippingprice" asp-action="Edit">
                <input type="hidden" asp-for="Id" />
                <div class="form-group ">
                    <label class="col-lg-2 control-label" asp-for="Country"></label>
                    <div class="col-lg-10">
                        <input placeholder="Product name" asp-for="Country" class="form-control">
                        <span class="text-danger" asp-validation-for="Country"></span>
                    </div>
                </div>

                <div class="form-group ">
                    <label class="col-lg-2 control-label" asp-for="Price"></label>
                    <div class="col-lg-10">
                        <input placeholder="Product quantity" asp-for="Price" class="form-control">
                        <span class="text-danger" asp-validation-for="Price"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-offset-2 col-lg-10">
                        <button class="btn btn-theme" type="submit">Update</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
