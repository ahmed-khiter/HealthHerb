@model PaymentSetting
@{
    Layout = "_AdminDashboard";
    ViewData["Title"] = "Payment setting";
}

<partial name="~/Views/Shared/_Alert.cshtml" />

<div class="row mt">
    <div class="col-lg-12">
        <h4><i class="fa fa-credit-card"></i> Stripe payment setting</h4>
        <div class="form-panel">
            <form class="form-horizontal style-form" method="post" asp-controller="setting" asp-action="paymentsetting">
                <input type="hidden" asp-for="Id" />
                <div class="form-group ">
                    <label class="col-lg-2 control-label" asp-for="ClientId"></label>
                    <div class="col-lg-10">
                        <input asp-for="ClientId" class="form-control">
                        <span class="text-danger" asp-validation-for="ClientId"></span>
                    </div>
                </div>
                <div class="form-group ">
                    <label class="col-lg-2 control-label" asp-for="ClientSecret"></label>
                    <div class="col-lg-10">
                        <input asp-for="ClientSecret" class="form-control">
                        <span class="text-danger" asp-validation-for="ClientSecret"></span>
                    </div>
                </div>
                <div class="form-group ">
                    <label class="col-lg-2 control-label" asp-for="IsLive"></label>
                    <div class="col-lg-10">
                        <input asp-for="IsLive">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-lg-offset-2 col-lg-10">
                        <button class="btn btn-theme" type="submit">Save</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>